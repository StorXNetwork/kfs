{
  "name": "kfs",
  "version": "0.0.0",
  "description": "a kademlia inspired local file store based on leveldb ",
  "main": "index.js",
  "bin": {
    "kfs": "bin/kfs.js",
    "kfsd": "bin/kfsd.js"
  },
  "scripts": {
    "test": "npm run testsuite && npm run linter",
    "testsuite": "mocha test/** --recursive",
    "linter": "jshint --config .jshintrc ./index.js ./lib ./test",
    "coverage": "istanbul cover _mocha -- --recursive",
    "generate-docs": "node_modules/.bin/jsdoc --configure .jsdoc.json --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bookchin/kfs.git"
  },
  "keywords": [
    "kad",
    "kademlia",
    "leveldb",
    "levelup",
    "leveldown",
    "file",
    "store",
    "system"
  ],
  "author": "Gordon Hall <gordon@gordonwritescode.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/bookchin/kfs/issues"
  },
  "homepage": "https://github.com/bookchin/kfs#readme",
  "dependencies": {
    "leveldown": "^1.4.6",
    "merge": "^1.2.0",
    "mkdirp": "^0.5.1",
    "readable-stream": "^2.1.5"
  },
  "devDependencies": {
    "coveralls": "^2.11.12",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.0",
    "jshint": "^2.9.3",
    "minami": "^1.1.1",
    "mocha": "^3.0.2"
  }
}
